{
  "name": "typescript-template",
  "version": "0.1.0",
  "description": "code only with typescript",
  "main": "dist/main.cjs",
  "types": "dist/main.d.ts",
  "module": "dist/main.mjs",
  "scripts": {
    "build:es": "tsc --outDir es",
    "build:umd": "rollup --config rollup.config.mjs",
    "build:commonjs": "tsc --module commonjs --outDir commonjs",
    "build": "pnpm build:es && pnpm build:umd && pnpm build:commonjs",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "devDependencies": {
    "eslint": "7.32.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vitest": "^0.27.2"
  },
  "author": "LuoAnbing",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "xml-js": "^1.6.11"
  }
}
